<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">  
 
<hibernate-mapping>
   <class name="RigaDiAcquisto" table="`RigaDiAcquisto`">
      <id name="id" column="`id`">
         <generator class="increment"/>
      </id>
      <property name="subtotale" column="`subtotale`"/>
      <property name="quantita" column="`quantita`"/>
      
      <many-to-one name="acquisto" column="`acquistoId`" not-null="true"/>
      <many-to-one name="libro" column="`libroId`" not-null="true"/>
   </class>
</hibernate-mapping>